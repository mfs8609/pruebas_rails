<h1>Editar Usuario</h1>
<%=form_for(@usuario) do |f|%>
	
	<div class="field">
		<%=f.label :nombre%><br/>
		<%=f.text_field :nombre%>
	</div>
	<div class="field">
		<%=f.label :email%><br/>
		<%=f.text_field :email%>
	</div>
	<div class="actions">
		<%=f.submit 'Guardar Cambios'%>
	</div>
<%end%>

<%= link_to 'Atras', users_path %>